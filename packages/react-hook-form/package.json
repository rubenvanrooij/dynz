{
  "name": "@dynz/react-hook-form",
  "version": "0.0.11",
  "private": false,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "typescriptn",
    "schema",
    "validation",
    "parsing",
    "form",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenvanrooij/dynz"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Ruben van Rooij <ruben.van.rooij@gmail.com>",
  "description": "React hook form integration for dynz.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest run --watch"
  },
  "peerDependencies": {
    "dynz": "^0.0.9",
    "@hookform/resolvers": "^5.0.0",
    "react-hook-form": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "react": "^19.2.0",
    "tsdown": "^0.14.2"
  }
}
