{
  "name": "dynz",
  "version": "0.0.11",
  "private": false,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "typescript",
    "schema",
    "validation",
    "parsing",
    "form",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenvanrooij/dynz"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Ruben van Rooij <ruben.van.rooij@gmail.com>",
  "description": "A powerful TypeScript schema validation with advanced conditional logic, cross-field validation, and static type inference.",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest run --watch"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "tsdown": "^0.16.5"
  }
}
