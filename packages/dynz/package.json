{
  "name": "dynz",
  "version": "0.0.9",
  "private": false,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "typescript",
    "schema",
    "validation",
    "parsing",
    "form",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenvanrooij/dynz"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Ruben van Rooij <ruben.van.rooij@gmail.com>",
  "description": "A powerful TypeScript schema validation with advanced conditional logic, cross-field validation, and static type inference.",
  "main": "./dist/dynz.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dynz.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dynz.cjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest run --watch"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4"
  }
}
